{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes",
      "labels": ["bug", "bugfix"]
    },
    {
      "title": "## 🛡️ Security",
      "labels": ["security"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🧪 Testing",
      "labels": ["test", "testing"]
    },
    {
      "title": "## ⚡ Performance",
      "labels": ["performance", "optimization"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["maintenance", "refactor", "cleanup"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": "ASC",
  "template": "${{CHANGELOG}}",
  "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "\\[(.*)\\]",
      "target": "$1"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[(.*)\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+)",
      "target": "- $4"
    }
  ]
}