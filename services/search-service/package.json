{
  "name": "search-service",
  "version": "1.0.0",
  "description": "Search service for the Threat Modeling Application with Elasticsearch integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t search-service .",
    "docker:run": "docker run -p 3010:3010 search-service",
    "elastic:setup": "node scripts/setup-elasticsearch.js"
  },
  "keywords": [
    "search-service",
    "elasticsearch",
    "full-text-search",
    "threat-modeling",
    "fastify",
    "typescript"
  ],
  "author": "Threat Modeling Team",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "natural": "^6.7.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}