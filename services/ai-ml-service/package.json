{
  "name": "@threatmodel/ai-ml-service",
  "version": "1.0.0",
  "description": "AI/ML service for intelligent threat analysis and recommendations",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "train": "ts-node src/scripts/train-models.ts",
    "train:all": "ts-node src/scripts/train-models.ts all",
    "train:threat": "ts-node src/scripts/train-models.ts threat-classifier",
    "train:vulnerability": "ts-node src/scripts/train-models.ts vulnerability-predictor",
    "train:mitigation": "ts-node src/scripts/train-models.ts mitigation-recommender",
    "train:pattern": "ts-node src/scripts/train-models.ts pattern-recognizer",
    "train:generate-data": "ts-node src/scripts/train-models.ts generate-data",
    "train:evaluate": "ts-node src/scripts/train-models.ts evaluate"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^1.10.1",
    "natural": "^6.10.0",
    "ml-knn": "^3.0.0",
    "ml-random-forest": "^2.1.0",
    "node-nlp": "^4.27.0",
    "compromise": "^14.10.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "zod": "^3.22.4",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.11",
    "axios": "^1.6.2",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "commander": "^11.1.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "bull": "^4.11.5",
    "prom-client": "^15.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/natural": "^5.1.5",
    "@types/express": "^4.17.21",
    "@types/bull": "^4.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.15"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.ts"],
    "testMatch": ["**/__tests__/**/*.test.ts", "**/tests/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/server.ts",
      "!src/ml-training/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "threat-modeling",
    "ai",
    "ml",
    "machine-learning",
    "security",
    "fastify",
    "typescript"
  ],
  "author": "Threat Modeling Team",
  "license": "MIT"
}